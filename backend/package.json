{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/config/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/config/index.js",
    "dev": "nodemon --exec ts-node src/config/index.ts",
    "dev:real": "node server-real.js",
    "dev:mock": "node server.js",
    "dev:test": "node server-test.js",
    "dev:scrape": "node server-real-scraping.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:mocked": "TEST_ENV=mocked jest tests/mock --runInBand --no-cache --coverage --coverageDirectory=coverage/mocked",
    "test:unmocked": "TEST_ENV=unmocked jest tests/unmock --runInBand --no-cache --coverage --coverageDirectory=coverage/unmocked",
    "coverage:merge": "npx nyc merge coverage coverage/merged && npx nyc report --report-dir=coverage/combined --reporter=html --reporter=text-summary",
    "coverage:all": "npm run test:mocked && npm run test:unmocked && npm run coverage:merge",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "node-geocoder": "^4.4.1",
    "openai": "^6.5.0",
    "puppeteer": "^24.23.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/node-geocoder": "^4.2.6",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.0",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.3"
  }
}
